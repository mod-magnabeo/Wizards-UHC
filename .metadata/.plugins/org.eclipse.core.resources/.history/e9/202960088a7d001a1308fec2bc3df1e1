	public int deDegréAPourcentage(int x) {
		//transfére degrés a pourcentage
		return x/360*100;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	public void createPlayer() {//start
		
		 boolean matchW=false;
		 int i;
		 int rando;
